{
  "format_version": "1.13.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "wormwood:other_surface_waterlily_feature",
      "places_feature": "minecraft:fixup_waterlily_position_feature"
    },
    "conditions": {
      "placement_pass": "surface_pass",
      "minecraft:biome_filter": [
        {
          "any_of": [
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "birch"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "bee_habitat"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "grove"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "meadow"
            },
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "plains"
            }
          ]
        }
      ]
    },
    "distribution": {
      "iterations": 1,
      "scatter_chance": {
        "numerator": 1,
        "denominator": 10
      },
      "x": {
        "distribution": "uniform",
        "extent": [ 0, 16 ]
      },
      "y": {
        "distribution": "uniform",
        "extent": [ 0, "query.heightmap(variable.worldx, variable.worldz) * 2" ]
      },
      "z": {
        "distribution": "uniform",
        "extent": [ 0, 16 ]
      }
    }
  }
}
